{
  "name": "bttrReactNative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test-unit": "jest",
    "test-unit-coverage": "jest --coverage",
    "build-android-e2e-debug": "detox build --configuration android.emu.debug",
    "test-android-e2e-debug": "detox test --configuration android.emu.debug --record-videos all",
    "build-ios-e2e-debug": "detox build --configuration ios.sim.debug",
    "test-ios-e2e-debug": "detox test --configuration ios.sim.debug",
    "prettify": "yarn prettier . --write",
    "prettify-check": "prettier . --check",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js src/**",
    "lint-fix": "eslint --ext .tsx,.ts,.jsx,.js src/** --fix",
    "package-clean": "rm -rf node_modules yarn.lock && yarn install",
    "commit-version": "git add CHANGELOG.md package.json && git commit -m 'update CHANGELOG and number version'",
    "build-link-ios": "cd ios && rm -rf Pods Podfile.lock && pod install && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@reduxjs/toolkit": "^2.0.1",
    "@rneui/base": "^4.0.0-rc.8",
    "@rneui/themed": "^4.0.0-rc.8",
    "react": "18.2.0",
    "react-native": "0.73.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.0.3",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@faker-js/faker": "^8.3.1",
    "@react-native/babel-preset": "^0.73.18",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": ">=6.0.0",
    "@typescript-eslint/parser": ">=6.0.0",
    "babel-jest": "^29.6.3",
    "eslint": ">=8.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^1.0.1",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29",
    "prettier": "3.1.1",
    "react-test-renderer": "18.2.0",
    "typescript": ">=4.7"
  },
  "engines": {
    "node": ">=18"
  }
}
